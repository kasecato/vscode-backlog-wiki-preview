{
	"Insert italic text": {
		"prefix": "italic",
		"body": "'''${1:${TM_SELECTED_TEXT}}'''$0",
		"description": "Insert italic text"
	},
	"Insert bold text": {
		"prefix": "bold",
		"body": "''${1:${TM_SELECTED_TEXT}}''$0",
		"description": "Insert bold text"
	},
	"Insert strike text": {
		"prefix": "strike",
		"body": "%%${1:${TM_SELECTED_TEXT}}%%",
		"description": "Insert strike text"
	},
	"Insert color text": {
		"prefix": "color",
		"body": "&color(${1|aqua,black,blue,fuchsia,gray,green,lime,maroon,navy,olive,orange,purple,red,silver,teal,white,yellow|}){${TM_SELECTED_TEXT}}$0",
		"description": "Insert color"
	},
	"Insert url": {
		"prefix": "url",
		"body": "[[${1:text}:https://${2:link}]]$0",
		"description": "Insert url"
	},
	"Insert header": {
		"prefix": "header",
		"body": "* ${1:text}",
		"description": "Insert header"
	},
	"Insert unordered list": {
		"prefix": "unordered list",
		"body": [
			"- ${1:first}",
			"- ${2:second}",
			"- ${3:third}",
			"$0"
		],
		"description": "Insert unordered list"
	},
	"Insert ordered list": {
		"prefix": "ordered list",
		"body": [
			"+ ${1:first}",
			"+ ${2:second}",
			"+ ${3:third}",
			"$0"
		],
		"description": "Insert ordered list"
	},
	"Insert quoted text": {
		"prefix": "quote",
		"body": "> ${1:${TM_SELECTED_TEXT}}",
		"description": "Insert quoted text"
	},
	"Insert code": {
		"prefix": "code",
		"body": [
			"{code}",
			"$0",
			"{/code}"
		],
		"description": "Insert code"
	},
	"Insert table of contents": {
		"prefix": "contents",
		"body": [
			"#contents",
			"$0"
		],
		"description": "Insert table of contents"
	},
	"Insert break": {
		"prefix": "break",
		"body": "&br;",
		"description": "Insert break"
	},
	"Insert cacoo diagram": {
		"prefix": "cacoo",
		"body": "#cacoo(${1:path},${2:width},${3:height},${4:theme},${5:sheetNo})",
		"description": "Insert cacoo diagram"
	},
	"Insert image": {
		"prefix": "image",
		"body": "#image(https://${1:link})$0",
		"description": "Insert image"
	},
	"Insert thumbnail": {
		"prefix": "thumbnail",
		"body": "#thumbnail(https://${1:link})$0",
		"description": "Insert thumbnail"
	},
	"Insert attached file": {
		"prefix": "attach",
		"body": "#attach(https://${1:link})$0",
		"description": "Insert attached file"
	}
}
